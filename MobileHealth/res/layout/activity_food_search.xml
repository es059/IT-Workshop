<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mobilehealth.FoodSearch"
    tools:ignore="MergeRootFrame" >
    
      <fragment
		        android:id="@+id/add_searchBar"
		        android:name="com.example.mobilehealth.ActionBarSearchBarFragment"
		        android:layout_width="match_parent"
		        android:layout_height="@dimen/actionbar_height"
		        android:layout_alignParentTop="true"
		        android:layout_centerHorizontal="true"
		        tools:layout="@layout/actionbar_searchbar_fragment"/>
		
		
		    <ListView
		        android:id="@+id/add_foodList"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:layout_below="@+id/add_searchBar"
		        android:layout_centerHorizontal="true"
		        android:longClickable="true"
		        />

		    <ImageButton
		        android:id="@+id/imageButton1"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_alignParentBottom="true" />

   </RelativeLayout>
